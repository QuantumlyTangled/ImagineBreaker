{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:9999"
      }
    }
  },
  "ImagineBreaker": {
    "Logging": {
      "UsageUpdates": {
        "status": true,
        "startDelayMin": 1,
        "intervalMin": 1
      }
    },
    "Performance": {
      "ForcedGC": {
        "status": true,
        "startDelayMin": 1,
        "intervalMin": 1
      }
    }
  },
  "AllowedHosts": "*"
}
